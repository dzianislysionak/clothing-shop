{
  "name": "clothing-shop-server",
  "version": "1.0.0",
  "engines": {
    "node": "12.16.0",
    "npm": "6.13.4"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "7.0.0",
    "express": "^4.18.2",
    "stripe": "6.28.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^1.19.1"
  },
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dzianislysionak/clothing-shop.git"
  },
